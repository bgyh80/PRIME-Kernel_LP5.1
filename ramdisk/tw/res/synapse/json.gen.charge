		unit:" mA",
		default:`cat /sys/kernel/fast_charge/usb_charge_level`,
		action:"generic /sys/kernel/fast_charge/usb_charge_level",
		values:[`while read values; do busybox printf "%s, \n" $values; done < /sys/kernel/fast_charge/usb_levels`]
